{
    "compilerOptions": {
        "module": "es2015",
        "target": "es2016",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "removeComments": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "outDir": "nextcloudappstore/core/static/public",
        "lib": [
            "es2015",
            "es2016",
            "dom"
        ]
    },
    "include": [
        "nextcloudappstore/core/static/assets/app/**/*"
    ]
}
